	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-30" left="10" height="16" width="90" follows="left|top">
		Chat logging:
	</text>	
	<text name="text_box13" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
