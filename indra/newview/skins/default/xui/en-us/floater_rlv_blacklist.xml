<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="restrained love blacklist" title="RestrainedLove Black List"
	can_close="true" can_drag_on_left="false" can_minimize="true" can_resize="false"
	min_width="500" min_height="304" width="500" height="304">
	<text name="info1" font="SansSerifSmall" mouse_opaque="true" halign="center"
	 bottom_delta="-38" left="10" height="16" width="480" follows="left|right|top">
		With this floater, you may prevent RestrainedLove from applying restrictions
	</text>
	<text name="info2" font="SansSerifSmall" mouse_opaque="true" halign="center"
	 bottom_delta="-16" left="10" height="16" width="480" follows="left|right|top">
		you do not want to be imposed on your avatar or on the use of your viewer. 
	</text>
	<text name="info4" font="SansSerifSmall" text_color="white" mouse_opaque="true" halign="center"
	 bottom_delta="-16" left="10" height="16" width="480" follows="left|right|top">
		The changes will only take effect after a viewer restart.
	</text>
	<check_box name="instantmessage" label="Forbid instant messaging restrictions"
	 font="SansSerifSmall" mouse_opaque="true"  enabled="true" tool_tip=" "
	 left="15" bottom_delta="-20" width="250" height="16" follows="left|top" />
