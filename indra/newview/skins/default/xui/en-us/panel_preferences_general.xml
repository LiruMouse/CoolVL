<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel border="true" bottom="-409" enabled="true" follows="left|top|right|bottom"
 height="408" label="General" left="102" mouse_opaque="true"
 name="general_panel" width="517">
	<text name="grid_selection_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
     bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom="-25" left="10" height="18" width="394" follows="left|top">
		Grid selection:
	</text>
	<check_box name="force_show_grid_checkbox" control_name="ForceShowGrid"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Show the grid selection combo on the login screen" font="SansSerifSmall"
	 bottom_delta="0" left="130" height="16" width="400" follows="left|top" />
	<text name="start_location_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-24" left="10" height="18" width="394" follows="left|top">
		Start Location:
	</text>
	<radio_group name="default_start_location" control_name="LoginLastLocation" draw_border="false"
	 bottom_delta="0" left="130" height="20" width="220" follows="left|top">
		<radio_item name="MyHome" bottom="-20" left="0" height="20" width="50"
		 tool_tip="Log into my home location by default.">
			My Home
		</radio_item>
		<radio_item name="MyLastLocation" bottom="-20" left="100" height="20" width="50"
		 tool_tip="Log into my last location by default.">
			My Last Location
		</radio_item>
	</radio_group>
	<check_box name="show_location_checkbox" control_name="ShowStartLocation"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Show Start Location on Login Screen" font="SansSerifSmall"
	 bottom_delta="-20" left="130" height="16" width="256" follows="left|top" />
	<text name="avatar_names_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-24" left="10" height="18" width="394" follows="left|top">
		Avatar Names:
	</text>
	<text name="show_names_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="0" left="130" height="18" width="50" follows="left|top">
		Show:
	</text>
	<combo_box name="fade_out_combobox" enabled="true" mouse_opaque="true"
	 bottom_delta="2" left="170" height="18" width="120" follows="left|top">
		<combo_item name="Never" value="default">
			Never
		</combo_item>
		<combo_item name="Show Temporarily" value="default">
			Show Temporarily
		</combo_item>
		<combo_item name="Always" value="default">
			Always
		</combo_item>
	</combo_box>
	<slider name="bubble_opacity" control_name="ChatBubbleOpacity"
	 can_edit_text="false" min_val="0" max_val="1" increment="0.05" decimal_digits="3"
	 show_text="true" label="Opacity" enabled="true" mouse_opaque="true"
	 tool_tip="Note that this setting is the same as the Chat Bubble Opacity in the Text Chat preferences"
	 bottom_delta="0" left="300" height="16" width="190" follows="left|top"/>
	<check_box name="small_avatar_names_checkbox" control_name="SmallAvatarNames"
	 label="Small Avatar Names" font="SansSerifSmall"
	 bottom_delta="-22" left="130" height="16" width="150" follows="left|top"/>
	<check_box name="show_my_name_checkbox" control_name="RenderNameHideSelf"
	 label="Hide My Name On My Screen" font="SansSerifSmall"
	 bottom_delta="0" left="300" height="16" width="200" follows="left|top"/>
	<view_border name="display_names_border" bevel_style="none" border_thickness="1"
	 bottom_delta="-128" left="130" height="125" width="345" follows="left|top"/>
	<text name="display_names_text_box1" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="113" left="138" height="10" width="350" follows="left|top">
		Avatar names displaying type for name tags, chat, IM, radar...
	</text>
	<text name="display_names_text_box2" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-12" left="138" height="10" width="350" follows="left|top">
		(the legacy name is always used when no display name exists):
	</text>
	<radio_group name="display_names_usage" control_name="DisplayNamesUsage"
	 draw_border="false" enabled="true" mouse_opaque="true"
	 bottom_delta="-60" left="135" height="55" width="325" follows="left|top">
		<radio_item name="legacy_names" type="string" length="1" enabled="true" mouse_opaque="true"
		 tool_tip="Example: 'John Doe' for an old resident (username: john.doe) or 'JohnDoe Resident' for a new resident (username: johndoe)"
		 left_delta="10" bottom="-18" height="18" width="100" follows="left|top">
			Legacy name
		</radio_item>
		<radio_item name="both_names" type="string" length="1" enabled="true" mouse_opaque="true"
		 tool_tip="Example: 'Johnny [John Doe]' for an old resident (username: john.doe) or 'Johnny [JohnDoe Resident]' for a new resident (username: johndoe) with their display name set to 'Johnny'"
		 bottom="-36" left_delta="0" height="18" width="100" follows="left|top">
			Display name [Legacy name]
		</radio_item>
		<radio_item name="display_names" type="string" length="1" enabled="true" mouse_opaque="true"
		 tool_tip="Example: 'Johnny' for any resident with their display name set to 'Johnny'"
		 bottom="-54" left_delta="0" height="18" width="100" follows="left|top">
			Display name
		</radio_item>
	</radio_group>
	<check_box name="legacy_names_for_friends_check" control_name="LegacyNamesForFriends"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Always use legacy name for friends list and notifications" font="SansSerifSmall"
	 tool_tip="The problem with display names, is that they can change... It's usually easier to keep track of your friends via their legacy name"
	 bottom_delta="-20" left="135" height="16" width="370" follows="left|top" />
	<check_box name="legacy_names_for_friends_check" control_name="OmitResidentAsLastName"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Never display 'Resident' as the last name in legacy names" font="SansSerifSmall"
	 tool_tip="With this option checked, new residents will not be listed with 'Resident' as their last name in their legacy name"
	 bottom_delta="-20" left="135" height="16" width="370" follows="left|top"/>
	<text name="group_titles_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-24" left="10" height="16" width="394" follows="left|top">
		Group Titles:
	</text>
	<check_box name="show_all_title_checkbox" control_name="RenderHideGroupTitleAll"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Hide All Group Titles" font="SansSerifSmall"
	 bottom_delta="0" left="130" height="16" width="150" follows="left|top"/>
	<check_box name="show_my_title_checkbox" control_name="RenderHideGroupTitle"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Hide My Group Title" font="SansSerifSmall"
	 bottom_delta="0" left="300" height="16" width="200" follows="left|top"/>
	<text name="effects_color_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-30" left="10" height="18" width="394" follows="left|top">
		Color For My Effects:
	</text>
	<text type="string" name="effects_color_explain_textbox" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-12" left="10" height="16" width="394" follows="left|top">
		(beam &amp; particles)
	</text>
	<color_swatch name="effect_color_swatch" control_name="EffectColor" enabled="true" mouse_opaque="true"
	 border_color="0.45098 0.517647 0.607843 1" can_apply_immediately="false" label=""
	 tool_tip="Click to open Color Picker"
	 bottom_delta="-16" left="133" height="48" width="32" follows="left|top" />
	<slider name="ui_scale_slider" control_name="UIScaleFactor" enabled="true" mouse_opaque="true"
	 can_edit_text="true" min_val="0.75" max_val="1.5" increment="0.025" decimal_digits="3"
	 show_text="true" label="UI size:"
	 bottom_delta="30" left="185" height="18" width="280" follows="left|top" />
	<check_box name="ui_auto_scale" control_name="UIAutoScale"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Use resolution independent scale" font="SansSerifSmall"
	 bottom_delta="-18" left_delta="45" height="18" width="230" follows="left|top"/>
	<spinner name="afk_timeout_spinner" control_name="AFKTimeout" enabled="true" mouse_opaque="true"
	 min_val="0" max_val="999" increment="1" decimal_digits="0"
	 tool_tip="Timeout in seconds (0 to disable). Any value between 1 and 30 is interpreted as 30s."
	 label="Away Timeout:" label_width="120"
	 bottom_delta="-24" left="10" height="16" width="170" follows="left|top"/>
	<text name="seconds_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="0" left="190" height="16" width="200" follows="left|top">
		seconds (0 to disable)
	</text>
	<text name="maturity_desired_label" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-28" left="10" height="18" width="394" follows="left|top">
		 Rating:
	</text>
	<text name="maturity_desired_prompt" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="0" left="135" height="18" width="250" follows="left|top">
		 I want to access content rated:
	</text>
	<combo_box name="maturity_desired_combobox" enabled="true" mouse_opaque="true"
	 bottom_delta="1" left="315" height="18" width="150" follows="left|top">
		<combo_item name="Desired_Adult" value="42">
			PG, Mature and Adult
		</combo_item>
		<combo_item name="Desired_Mature" value="21">
			PG and Mature
		</combo_item>
		<combo_item name="Desired_PG" value="13">
			PG only
		</combo_item>
	</combo_box>
	<text name="maturity_desired_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-1" left="315" height="18" width="150" follows="left|top">
		 PG only
	</text>
	<text name="language_textbox" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="-28" left="10" height="18" width="394" follows="left|top">
		Language:
	</text>
	<text name="language_textbox2" type="string" length="1" enabled="true" mouse_opaque="true"
	 bg_visible="false" border_drop_shadow_visible="false" border_visible="false" drop_shadow_visible="true"
	 font="SansSerifSmall" v_pad="0" h_pad="0" halign="left"
	 bottom_delta="0" left="293" height="18" width="400" follows="left|top">
		(Requires restart for full effect)
	</text>
	<combo_box name="language_combobox" enabled="true" mouse_opaque="true"
	 allow_text_entry="true" max_chars="135"
	 bottom_delta="4" left="132" height="16" width="146" follows="left|top">
		<combo_item type="string" length="1" enabled="true" name="System Default Language" value="default">
			System Default
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="English" value="en-us">
			English
		</combo_item>
		
		<!-- After "System Default" and "English", please keep the rest of these combo_items in alphabetical order by the first character in the string. -->
		
		<combo_item type="string" length="1" enabled="true" name="Danish" value="da">
			Dansk (Danish) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Deutsch(German)" value="de">
			Deutsch (German) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Spanish" value="es">
			Español (Spanish) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="French" value="fr">
			Français (French) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Italian" value="it">
			Italiano (Italian) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Hungarian" value="hu">
			Magyar (Hungarian) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Dutch" value="nl">
			Nederlands (Dutch) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Polish" value="pl">
			Polski (Polish) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Portugese" value="pt">
			Portugués (Portuguese) - Beta
		</combo_item>
 		<combo_item type="string" length="1" enabled="true" name="Russian" value="ru">
 			Русский (Russian) - Beta
 		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Turkish" value="tr">
			Türkçe (Turkish) - Beta
		</combo_item>
 		<combo_item type="string" length="1" enabled="true" name="Ukrainian" value="uk">
 			Українська (Ukrainian) - Beta
 		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="Chinese" value="zh">
			中文 (简体) (Chinese) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="(Japanese)" value="ja">
			日本語 (Japanese) - Beta
		</combo_item>
		<combo_item type="string" length="1" enabled="true" name="(Korean)" value="ko">
			한국어 (Korean) - Beta
		</combo_item>
	</combo_box>
	<string name="region_name_prompt">
		&lt;Type region name&gt;
	</string>
	<check_box name="language_is_public" control_name="LanguageIsPublic"
	 radio_style="false" enabled="true" mouse_opaque="true"
	 label="Share language with objects" font="SansSerifSmall"
	 tool_tip="This lets in-world objects know your preferred language."
	 bottom_delta="-24" left="130" height="18" width="256" follows="left|top"/>
</panel>
